{
    "name": "fhusquinet/laravel-model-json-options",
    "description": "Store options as JSON values easily on your models.",
    "keywords": [
        "fhusquinet",
        "laravel-model-json-options"
    ],
    "homepage": "https://github.com/fhusquinet/laravel-model-json-options",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Husquinet",
            "email": "florian.husquinet@deegital.be",
            "homepage": "https://www.deegital.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "FHusquinet\\ModelOptions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FHusquinet\\ModelOptions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FHusquinet\\ModelOptions\\ModelOptionsServiceProvider"
            ]
        }
    }
}
